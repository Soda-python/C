#include <stdio.h>
unsigned long long factorial(int n) {
    if (n == 0 || n == 1) {
        return 1;  
    } else {
        return n * factorial(n - 1);
    }
}
int main() 
{
    int m,n;
    scanf("%d %d",&m,&n);
    printf("result = %d",factorial(n)/factorial(m)/factorial(n-m));
    return 0;
}
