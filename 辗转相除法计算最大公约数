//以下包含求最小公倍数
#include<stdio.h>
int main(){
	int a,b,t,aa,bb;
	scanf("%d %d",&a,&b);
	aa = a;
	bb = b;
	while (b!=0){
		t = a % b;
		a = b;
		b = t;
	}
	int lcm;
	lcm = (aa*bb) / a;
	printf("gcd is %d\n",a);
	printf("lcm is %d",lcm);
	return 0;
}
